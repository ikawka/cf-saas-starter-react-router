{
  "name": "{{projectName}}",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "deploy": "bun run build && wrangler deploy",
    "dev": "bun run db:migrate:local && react-router dev",
    "postinstall": "npm run cf-typegen",
    "preview": "bun run build && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "typegen": "wrangler types && react-router typegen",
    "auth:generate": "bunx --bun @better-auth/cli generate --config auth.config.ts",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply {{dbName}} --local",
    "db:migrate:remote": "wrangler d1 migrations apply {{dbName}} --remote",
    "setup": "bun run scripts/first-time-setup.ts",
    "teardown": "bun run scripts/teardown.ts",
    "test:e2e": "bunx playwright test",
    "test:e2e:ui": "bunx playwright test --ui",
    "test:e2e:report": "bunx playwright show-report"
  },
